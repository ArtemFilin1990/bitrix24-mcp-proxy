# Bitrix24 MCP Proxy Environment Variables

# Application settings
NODE_ENV=development
PORT=3000

# Bitrix24 OAuth credentials (for cloud apps)
B24_CLIENT_ID=your_client_id_here
B24_CLIENT_SECRET=your_client_secret_here

# Bitrix24 webhook URL (for self-hosted or webhook integration)
B24_WEBHOOK_URL=https://your-domain.bitrix24.ru/rest/1/your_webhook_key/

# Logging level (debug, info, warn, error)
LOG_LEVEL=info
